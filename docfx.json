{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "templates/minimal"],
    "globalMetadata": {
      "_appName": "MyCSharpDocs",
      "_appTitle": "MyCSharpDocs",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
